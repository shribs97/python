    "a=int(input(\"Enter a number\"))\n",
    "b=a\n",
    "rem=0\n",
    "rev=0\n",
    "while(a>0):\n",
    "    rem=a%10\n",
    "    rev=(rev*10)+(rem)\n",
    "    a//=10\n",
    "print(str(rev))\n",
    "if(b==rev):\n",
    "    print(\"Number is Pallindrome\")\n",
    "else:\n",
    "    print(\"Number is not Pallindrome\")"
   ]
